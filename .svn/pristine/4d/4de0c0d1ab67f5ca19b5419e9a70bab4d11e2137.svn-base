#import "AppUtil.h"

@implementation AppUtil

+ (NSString *)getAppPath
{
    return [[NSBundle mainBundle]resourcePath];
}

+ (NSString *) getCachesPath
{
    return [NSString stringWithFormat:@"%@/Library/Caches/",NSHomeDirectory()];
}
@end
